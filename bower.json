{
  "name": "Tadpole.js",
  "version": "0.0.1",
  "homepage": "http://tadpole.sourcetoad.com",
  "authors": [
    "Sourcetoad"
  ],
  "description": "backbonejs mvc-style starter kit",
  "main": "app/index.html",
  "license": "MIT",
  "keywords": [
    "backbone.js",
    "backbone",
    "grunt",
    "mvc",
    "Sourcetoad",
    "underscore",
    "jquery"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "async": "~0.9.0",
    "backbone": "~1.1.2",
    "handlebars": "~2.0.0",
    "jquery": "~2.1.1"
  },
  "exportsOverride": {
    "bootstrap-*": {
      "js": "**/*.js",
      "css": "**/*.css"
    },

    "*": {
      "js": "**/*.js",
      "css": "**/*.css"
    }
  }
}
